	<queryString>
		<![CDATA[SELECT * from CheckInOut
INNER JOIN Customer USING (CustID)
INNER JOIN RoomBooking ON CheckInOut.ResvNo = RoomBooking.ResvNo
INNER JOIN Room ON RoomBooking.RoomNo = Room.RoomNo
INNER JOIN RoomType ON Room.RoomTypeID = RoomType.TypeID
WHERE CheckInOut.CheckInDate = date('now')]]>
	</queryString>
